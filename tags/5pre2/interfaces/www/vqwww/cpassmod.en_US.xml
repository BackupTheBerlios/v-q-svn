<?xml version="1.0" encoding="iso-8859-1"?>
<msgs>
	<menu>
		<mod>
			<m n="pass">Change password</m>
			<desc>
				<m n="pass">
					Remeber to change password from time to time.
					Using the same password for a long time
					may easy taking over your account by unauthorized
					person.
				</m>
			</desc>
		</mod>
	</menu>
	<mod>
		<pass>
			<m n="inv/done">Password was changed.</m>
			<m n="inv/cpass/@empty">Specify current password.</m>
			<m n="inv/npass/@empty">Specify new password.</m>
			<m n="inv/npass/@dirty">New password includes invalid characters.</m>
			<m n="inv/npass/@tooshort">New password is too short.</m>
			<m n="inv/npass/@toolong">New password is too long.</m>
			<m n="inv/dontmatch">New password and repeat mismatch.</m>
			<m n="inv/chgcant">Can't change the password.</m>
			<m n="inv/getcant">Can't read current password.</m>
			<m n="inv/oldbad">Invalid current password.</m>
			<m n="submit">Change</m>
			<m n="cpass">Current password:</m>
			<m n="npass">New password:</m>
			<m n="rpass">Repeat new password:</m>
		</pass>
	</mod>
</msgs>
